    TS_ASSERT(!field.valid());
    intValField.value() = 1U;
    TS_ASSERT(field.valid());
    enumValField.value() = Enum1_NumOfValues;
    TS_ASSERT(!field.valid());
    enumValField.value() = Enum1_Value1;
    TS_ASSERT(field.valid());
    constValField.value() = 10;
    TS_ASSERT(!field.valid());
    constValField.value() = 100;
    TS_ASSERT(field.valid());

